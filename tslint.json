{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "member-access": false,
    "ordered-imports": false,
    "interface-name": false,
    "no-empty-interface": false,
    "object-literal-sort-keys": false,
    "max-classes-per-file": false,
    "no-console": {
      "severity": "warning",
      "options": [
        "info",
        "log",
        "time",
        "timeEnd",
        "trace"
      ]
    }
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
