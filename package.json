{
  "name": "proxyer_web",
  "version": "0.1.0",
  "dependencies": {
    "@types/query-string": "^6.1.0",
    "antd": "^3.6.1",
    "axios": "^0.18.0",
    "clipboard": "^2.0.1",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "query-string": "^6.1.0",
    "react": "^16.4.0",
    "react-app-rewire-less": "^2.1.1",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "^2.16.0",
    "redux": "^4.0.0",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "proxy": "http://localhost:8081",
  "scripts": {
    "postinstall": "rm -rf node_modules/@types/node",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-app-rewired start --scripts-version react-scripts-ts",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-app-rewired build --scripts-version react-scripts-ts",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/clipboard": "^2.0.1",
    "@types/immutable": "^3.8.7",
    "@types/jest": "^23.0.0",
    "@types/prop-types": "^15.5.3",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.2",
    "@types/react-router": "^4.0.26",
    "@types/react-router-dom": "^4.2.7",
    "@types/redux": "^3.6.0",
    "@types/redux-devtools-extension": "^2.13.2",
    "@types/redux-immutable": "^3.0.38",
    "@types/why-did-you-update": "^0.0.8",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react-app-rewired": "^1.5.2",
    "redux-devtools-extension": "^2.13.2",
    "ts-import-plugin": "^1.5.2",
    "typescript": "^2.9.1",
    "why-did-you-update": "^0.1.1"
  },
  "author": {
    "name": "Yu Tian",
    "email": "tianyu@juxinli.com"
  }
}
